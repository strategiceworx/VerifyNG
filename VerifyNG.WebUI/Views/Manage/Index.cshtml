@{
    ViewBag.Title = "Home Page";
}

<div class="section section-white">
    <div class="verify-container" style="width: 97%;">
        <div class="row">
            <!-- Sidebar -->
            <div class="col-sm-2 blog-sidebar" style="padding-left:5px;padding-right:4px">
                <h4 style="margin-bottom: 5px;">Actions</h4>
                <div id="ctl00_menucontainer" role="navigation" class="navcollapse">
                    <ul id="accordion" class="accordion">
                        <li>
                            <a href="corpHome" id="ctl00_ahome" style="text-decoration: none">
                                <div class="link"><i class="glyphicon glyphicon-hand-right"></i>My Dashboard</div>
                            </a>
                        </li>
                        <li>
                            <a href="CorpChangePassword" id="ctl00_achangepass" style="text-decoration: none">
                                <div class="link"><i class="glyphicon glyphicon-hand-right"></i>Change Password</div>
                            </a>
                        </li>

                        <li>
                            <a href="CourseCorrection.aspx" id="ctl00_a1" style="text-decoration: none">
                                <div class="link"><i class="glyphicon glyphicon-hand-right"></i>Fingerprint Profile</div>
                            </a>
                        </li>

                        <li>
                            <a href="CourseCorrection.aspx" id="ctl00_a1" style="text-decoration: none">
                                <div class="link"><i class="glyphicon glyphicon-hand-right"></i>Update Profile</div>
                            </a>
                        </li>
                        <li>
                            <a href="home?logout=true" id="ctl00_alogout" style="text-decoration: none">
                                <div class="link"><i class="glyphicon glyphicon-hand-right"></i>Log out</div>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-sm-10">
                <div class="row">
                    <div class="col-sm-6" style="margin-bottom: 2px;">
                        <h4>
                            <span class="label label-success" style="font-weight:normal;">08012345678</span>
                        </h4>
                    </div>
                    <div class="col-sm-6" style="text-align: right;">
                        <h4 class="comment-date">
                            <span id="ctl00_lblDate">Today's Date: Thursday, December 07, 2017</span>
                        </h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-8">
                        <form class="form-profile">

                            <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                                <div class="panel panel-success">
                                    <div class="panel-heading" role="tab" id="headingOne">
                                        <h4 class="panel-title">
                                            <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                                Personal Details <span class="glyphicon glyphicon-pencil"></span><span>(required)</span>
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div id="avatar">
                                                    <a href="javascript:void(0);" id="add_photo" class="add-more-article" style="display:block;font-size:1em;">
                                                        <img src="~/Images/120x120.png" class="img-thumbnail" id="candidate_photo" width="120" alt="" border="0">
                                                        @*<br>
                                                            <span>Add image</span>*@
                                                    </a>
                                                    @*<a href="javascript:void(0);" id="remove_photo" title="x remove">x</a>*@
                                                </div>

                                                <div class="content">
                                                    <div class="form-row move-right">
                                                        <div class="form-group col-md-6">
                                                            <label for="inputEmail4">Email</label>
                                                            <input type="email" class="form-control" id="inputEmail4" placeholder="Email">
                                                        </div>
                                                        <div class="form-group col-md-6">
                                                            <label for="inputPassword4">Password</label>
                                                            <input type="password" class="form-control" id="inputPassword4" placeholder="Password">
                                                        </div>
                                                    </div>
                                                    <div class="move-right">
                                                        <div class="form-group col-md-12">
                                                            <label for="inputAddress">Address</label>
                                                            <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St">
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label for="inputAddress2">Address 2</label>
                                                        <input type="text" class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor">
                                                    </div>
                                                    <div class="form-row">
                                                        <div class="form-group col-md-6">
                                                            <label for="inputCity">City</label>
                                                            <input type="text" class="form-control" id="inputCity">
                                                        </div>
                                                        <div class="form-group col-md-4">
                                                            <label for="inputState">State</label>
                                                            <select id="inputState" class="form-control">
                                                                <option selected="">Choose...</option>
                                                                <option>...</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group col-md-2">
                                                            <label for="inputZip">Zip</label>
                                                            <input type="text" class="form-control" id="inputZip">
                                                        </div>
                                                    </div>
                                                    <button type="submit" class="btn btn-primary" style="margin-left: 15px;">Sign in</button>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="panel panel-success">
                                    <div class="panel-heading" role="tab" id="headingTwo">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                                Work Experience <span class="glyphicon glyphicon-pencil"></span><span>(required)</span>
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="content">
                                                    <div class="form-group col-md-6">
                                                        <label for="inputEmail4">Email</label>
                                                        <input type="email" class="form-control" id="inputEmail4" placeholder="Email">
                                                    </div>
                                                    <div class="form-group col-md-6">
                                                        <label for="inputPassword4">Password</label>
                                                        <input type="password" class="form-control" id="inputPassword4" placeholder="Password">
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label for="inputAddress">Address</label>
                                                        <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St">
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label for="inputAddress2">Address 2</label>
                                                        <input type="text" class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor">
                                                    </div>
                                                    <button type="submit" class="btn btn-primary" style="margin-left: 15px;">Sign in</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel panel-success">
                                    <div class="panel-heading" role="tab" id="headingThree">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                                Education <span class="glyphicon glyphicon-pencil"></span><span>(required)</span>
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                                        <div class="panel-body">
                                            <div class="row">
                                                <div class="content">
                                                    <div class="form-group col-md-6">
                                                        <label for="inputEmail4">Email</label>
                                                        <input type="email" class="form-control" id="inputEmail4" placeholder="Email">
                                                    </div>
                                                    <div class="form-group col-md-6">
                                                        <label for="inputAddress">&nbsp;</label>
                                                        <p class="form-control">&nbsp;</p>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label for="inputAddress">Address</label>
                                                        <input type="text" class="form-control" id="inputAddress" placeholder="1234 Main St">
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label for="inputAddress2">Address 2</label>
                                                        <input type="text" class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor">
                                                    </div>
                                                    <button type="submit" class="btn btn-primary" style="margin-left: 15px;">Sign in</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel panel-success">
                                    <div class="panel-heading" role="tab" id="headingThree">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseFour" aria-expanded="false" aria-controls="collapseFour">
                                                Current Salary <span class="glyphicon glyphicon-pencil"></span><span>(required)</span>
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseFour" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingFour">
                                        <div class="panel-body">
                                            Anim
                                        </div>
                                    </div>
                                </div>
                                <div class="panel panel-success">
                                    <div class="panel-heading" role="tab" id="headingFive">
                                        <h4 class="panel-title">
                                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseFive" aria-expanded="false" aria-controls="collapseFive">
                                                Attachment <span class="glyphicon glyphicon-pencil"></span><span>(required)</span>
                                            </a>
                                        </h4>
                                    </div>
                                    <div id="collapseFive" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                                        <div class="panel-body">
                                            Anim
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>

                        <fieldset class="seperator manual"><legend align="center">Finished ?</legend></fieldset>

                    </div>
                    <div class="col-md-3 col-md-offset-1">
                        <nav class="form-sidebar hidden-print hidden-sm hidden-xs affix">
                            <ul class="nav bs-docs-sidenav">
                                <li class="">
                                    <a href="#glyphicons"><span class="glyphicon glyphicon-pencil"></span> Glyphicons</a>
                                </li>
                                <li class="active">
                                    <a href="#dropdowns"><span class="glyphicon glyphicon-pencil"></span> Dropdowns</a>
                                </li>
                                <li class="">
                                    <a href="#btn-groups"><span class="glyphicon glyphicon-pencil"></span> Button groups</a>
                                </li>
                                <li>
                                    <a href="#btn-dropdowns"><span class="glyphicon glyphicon-pencil"></span> Button dropdowns</a>
                                </li>
                                <li><a href="#wells"><span class="glyphicon glyphicon-pencil"></span> Wells</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


